{
  "manifest_version": 3,
  "name": "STOP Lažnim Vestima",
  "version": "1.0",
  "description": "Automatski uklanja neželjene sajtove iz rezultata, tokom google pretrage.",
  "permissions": [
    "storage",
    "webNavigation",
    "scripting"
  ],
  "host_permissions": [
    "https://www.google.com/search*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}